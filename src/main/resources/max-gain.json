    "name": "maxGain",
    "return": {
      "type": "java.lang.Integer",
      "comment": " Max gain"
    },
    "parameters": [
      {
        "name": "arr",
        "type": "[I",
        "comment": "An array of Integer"
      }
    ]
  },
